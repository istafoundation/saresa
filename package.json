{
  "name": "saresa",
  "version": "1.0.12",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "version:patch": "npm version patch --no-git-tag-version && node ./scripts/sync-version.js",
    "version:minor": "npm version minor --no-git-tag-version && node ./scripts/sync-version.js",
    "version:major": "npm version major --no-git-tag-version && node ./scripts/sync-version.js",
    "build:release": "npm run version:patch && node ./scripts/build-release.js",
    "build:release:minor": "npm run version:minor && node ./scripts/build-release.js",
    "build:release:major": "npm run version:major && node ./scripts/build-release.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-community/slider": "^5.0.1",
    "babel-preset-expo": "^54.0.9",
    "convex": "^1.31.4",
    "expo": "~54.0.31",
    "expo-audio": "~1.1.1",
    "expo-auth-session": "^7.0.10",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.13",
    "expo-crypto": "^15.0.8",
    "expo-haptics": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "^15.0.10",
    "lottie-react-native": "~7.3.1",
    "moti": "^0.30.0",
    "react": "19.1.0",
    "react-dom": "^19.2.3",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-worklets": "^0.7.1",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
